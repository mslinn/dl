  "version": "0.2.0",
  "configurations": [
    {
      "name": "PyTest All",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true
    },
    {
      "args": ["--ff", "--lf"],
      "name": "PyTest New and Failing",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true
    },
    {
      "name": "Python: Debug Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "args": [
        "-h"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "name": "dl help",
      "program": "src/dl.py",
      "request": "launch",
      "type": "python",
    },
    {
      "args": [
        "https://www.youtube.com/watch?v=BaW_jenozKc"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "name": "dl video",
      "program": "src/dl.py",
      "request": "launch",
      "type": "python",
    },
    {
      "args": [
        "-x",
        "https://www.youtube.com/watch?v=BaW_jenozKc"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "name": "dl -x",
      "program": "src/dl.py",
      "type": "python",
      "request": "launch",
    }
  ]
}
